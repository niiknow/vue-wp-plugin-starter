{
  "name": "vue-wp-plugin-starter",
  "version": "0.1.0",
  "description": "A WordPress Vue.js starter plugin",
  "author": "Tom Noogen <tommywhen@yahoo.com>",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "lint": "tslint --project ./tsconfig.json src/**/*.ts && eslint src/**/*.vue",
    "lint-fix": "tslint --project ./tsconfig.json --fix src/**/*.ts && eslint --fix src/**/*.vue",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.4",
    "@types/jest": "^26.0.24",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.11",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^10.3.4",
    "axios": "^0.21.4",
    "browser-sync": "^2.27.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "core-js": "^3.17.3",
    "css-loader": "5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "26.6.3",
    "jsdom": "^16.7.0",
    "laravel-mix": "^6.0.29",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "purecss-sass": "^2.0.6",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "ts-jest": "26.5.6",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2",
    "vue": "^3.2.11",
    "vue-axios": "^3.3.6",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.5.0",
    "vue-router": "^4.0.10",
    "webpack": "^5.52.0"
  }
}
